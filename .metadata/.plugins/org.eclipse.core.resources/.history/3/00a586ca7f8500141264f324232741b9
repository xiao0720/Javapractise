package tutorial1;

public class Loops {
	//print "Hello Worlds" for 5 times
	public static void main(String[] args) {
		for (int i = 0; i < 5; i++) {
			System.out.print("Hello Worlds");
			System.out.println(" Message" + (i+1));
		}
		for (int i = 9; i>=0; i--) {
			System.out.println("Counter i now is " + i);
		}
	}
}


